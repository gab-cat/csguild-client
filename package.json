{
  "name": "csguild-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "ci": "bun run type-check && bun run lint && bun run build",
    "api:generate": "docker run --rm -v ${PWD}:/local openapitools/openapi-generator-cli:v6.6.0 generate -i http://host.docker.internal:3001/api-docs-json -g typescript-axios -o /local/generated/api-client --additional-properties=npmName=csguild-api-client,npmVersion=1.0.0,supportsES6=true,withInterfaces=true,modelPropertyNaming=camelCase,enumPropertyNaming=UPPERCASE,useSingleRequestParameter=false,withSeparateModelsAndApi=true,apiPackage=api,modelPackage=models --skip-validate-spec",
    "api:generate:prod": "docker run --rm -v ${PWD}:/local openapitools/openapi-generator-cli:v6.6.0 generate -i https://csguild.tech/api-docs-json -g typescript-axios -o /local/generated/api-client --additional-properties=npmName=csguild-api-client,npmVersion=1.0.0,supportsES6=true,withInterfaces=true,modelPropertyNaming=camelCase,enumPropertyNaming=UPPERCASE,useSingleRequestParameter=false,withSeparateModelsAndApi=true,apiPackage=api,modelPackage=models --skip-validate-spec",
    "api:clean": "rm -rf ./generated/api-client",
    "api:regenerate": "bun run api:clean && bun run api:generate",
    "api:regenerate:prod": "bun run api:clean && bun run api:generate:prod"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-spring/web": "^10.0.1",
    "@react-three/drei": "^10.4.2",
    "@react-three/fiber": "^9.2.0",
    "@splinetool/react-spline": "^4.0.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/js-cookie": "^3.0.6",
    "@types/three": "^0.178.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.0",
    "js-cookie": "^3.0.5",
    "lenis": "^1.3.4",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "react-spring": "^10.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "three": "^0.178.0",
    "zod": "^3.25.74",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@openapitools/openapi-generator-cli": "^2.21.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  }
}
